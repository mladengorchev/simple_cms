<% @page_title = "Show Section" %>

<table>
  <tr>
    <td>Name</td><td> <%= @section.name %></td>
  </tr>
  <tr>
    <td>Position</td><td> <%= @section.position %></td>
  </tr>
  <tr>
    <td>Content</td><td> <%= @section.content %></td>
  </tr>
  <tr>
    <td>Content Type</td><td> <%= @section.content_type %></td>
  </tr>
  <tr>
    <td>Visible</td><td> <%= @section.visible ? 'true' : 'false' %></td>
  </tr>
  <tr>
    <td>Created on</td><td> <%= @section.created_at %></td>
  </tr>
</table>
<%= link_to("Back to list", {:action => 'index'}) %>
